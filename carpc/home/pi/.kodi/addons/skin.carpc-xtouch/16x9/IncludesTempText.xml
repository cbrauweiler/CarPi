<?xml version="1.0" encoding="UTF-8"?>

<!--  Display Temp-Sensor Output  -->

<includes>	
	<include name="TempText1">
		<control type="grouplist">
			<orientation>horizontal</orientation>
			<left>50</left>
			<top>20</top>
			<width>610</width>
			<height>55</height>
				<control type="label">
					<description>Temp Top Left</description>
					<posx>45</posx>
					<posy>10</posy>
					<height>40</height>
					<width min="100" max="500">auto</width>
					<label>[B]$INFO[Window(Home).Property(field1)]: $INFO[Window(Home).Property(Temp1)] $INFO[Window(Home).Property(Text1)]  [/B]</label> 
					<align>left</align>
					<aligny>center</aligny>
					<font>font30</font>
					<textcolor>$VAR[CustomColorNofocus]</textcolor>
                    <shadowcolor>black</shadowcolor>
				</control> 
				<control type="image">
					<description>Temp Trend</description>
					<left>20</left>
					<posy>16</posy>
					<width>35</width>
					<height>35</height>
					<texture>$INFO[Window(Home).Property(Trend1)]</texture>
				</control>
		</control>
	</include>    
